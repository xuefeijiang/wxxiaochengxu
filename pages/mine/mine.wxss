/* pages/mine/mine.wxss */
page{
    background: #f4f4f4;
}
.weui-media-box{
    margin: 12px 0;
    background: white;
    height: 60px;
    padding: 5px;
    overflow: hidden
}
.weui-photo{
  display: block;
  height: 55px;
  width: 55px;
  float: left;
  margin-right: 10px;
}
.weui-title{
  margin-bottom: 5px;
}
.weui-cell{
  background: white;
  border-bottom: 1px solid #707070;
  border-top: 1px solid #707070;
  margin: 12px 0;
  padding: 10px;
}
.active{
  padding: 10px 5px;
  overflow: hidden;
}
.conter{
   border-bottom: 1px solid #707070;
  border-top: 1px solid #707070;
}
.weui-cell__bd{
  display: inline;
    color: #000;
    font-size: 14px;
    padding-left: 15px;
}
.weui-cell__ft{
  float: right;
    font-size: 12px;
    color: #999;
}
.image{
  margin-right: 10px;
  vertical-align: middle;
  width:20px; height: 20px;
  float: left;
}
.map{
   border-top: 1px solid #707070;
}
